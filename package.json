    "@types/node": "^20.19.9",
    "@types/nodemailer": "6.4.14",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "4.0.1",
    "@types/pug": "^2.0.10",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
