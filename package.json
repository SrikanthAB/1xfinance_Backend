    "@casl/mongoose": "^8.0.3",
    "@faker-js/faker": "^9.7.0",
    "@getbrevo/brevo": "^2.2.0",
    "@googlemaps/google-maps-services-js": "^3.4.1",
    "axios": "^1.9.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "country-state-city": "^3.2.1",
    "dotenv": "^16.4.7",
    "ethers": "^5.7.2",
    "express": "^5.0.1",
    "express-validator": "^7.2.1",
    "express-zod-api": "^22.11.2",
    "google-auth-library": "^9.15.1",
    "http-status": "^2.1.0",
    "http-status-codes": "^2.3.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongoose": "^8.12.1",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "nodemailer": "^6.10.0",
    "pug": "^3.0.3",
    "smile-identity-core": "^3.1.0",
    "smtp-connection": "^4.0.2",
    "typescript": "^5.8.3",
    "uuid": "^9.0.1",
    "validator": "^13.12.0",
    "winston": "^3.17.0",
    "zod": "^3.24.2"
