{
  "name": "ownmali-backend-v2",
  "version": "1.0.0",
  "scripts": {
    "start": "yarn serve",
    "dev": "npx nodemon --exec npx ts-node src/server.ts",
    "build-ts": "cross-env NODE_OPTIONS=--max_old_space_size=4096 tsc",
    "build": "yarn build-ts",
    "serve": "node dist/server.js",
    "watch-node": "sleep 5 && npx nodemon dist/server.js",
    "watch-ts": "npx tsc -w",
    "watch": "npx concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"yarn watch-ts\" \"yarn watch-node\"",
    "lint": "npx eslint . --ext .ts"
  },
